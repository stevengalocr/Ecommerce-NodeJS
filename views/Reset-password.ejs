<%- include('./partials/header') %>


     <!-- ===== Main Section ===== -->
    <section class="login-page fullscreen overlay-blue">
            <!-- Main Content -->
            <div class="login-box">
                    <%- include('./partials/flash') %>
                <!-- Start of Login Form -->
               <form action="" method="POST" id="login-form">
                 <div class="text-center">
                       <h3 class="uppercase">Restablecer contraseña</h3>
                </div>
                    <input type="password" id="password" name="password" class="form-control mt15" placeholder="Contraseña">
                   <input type="password" id="passwordConfirm" name="passwordConfirm" class="form-control mt15" placeholder="Confirmar contraseña">

                    <div class="mt15 text-center">
                        <button class="btn btn-border btn-blue">Restablecer</button>
                    </div>
                    
                </form>
                <!-- End of Login Form -->

            </div>
            <!-- End of Main Content -->
        </section>
        <!-- ===== End of Main Section =====  -->


